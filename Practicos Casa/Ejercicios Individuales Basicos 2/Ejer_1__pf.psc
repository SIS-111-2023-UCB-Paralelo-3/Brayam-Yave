Proceso Ejer_1
	Definir chicos,chicas,total,num1,num2 Como ReaL;
	Escribir "Escriba la cantidad total de niños";
	Leer chicos;
	Escribir "Escriba la cantidad total de niñas";
	Leer chicas; 
	total <- chicos+chicas;
	num1 <- chicos/total*100;
	num2 <- chicas/total*100;
	Escribir "Los chicos tienen un porcentaje de: ",num1,"% ","y las chicas de: ",num2,"%";
FinProceso