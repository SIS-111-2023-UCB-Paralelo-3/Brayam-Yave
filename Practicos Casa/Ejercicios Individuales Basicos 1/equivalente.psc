Proceso equivalente
	Definir metros, centimetros, mm, pulgadas Como Real;
	Escribir "Ingrese los metros";
	Leer metros;
	centimetros<- metros*100;
	mm<- metros*1000;
	pulgadas<- metros*39.37;
	Escribir "metros convertidos a centimetros ",centimetros;
	Escribir "metros convertidos a mm ", mm;
	Escribir "metros convertidos a pulgadas ",pulgadas;
FinProceso
